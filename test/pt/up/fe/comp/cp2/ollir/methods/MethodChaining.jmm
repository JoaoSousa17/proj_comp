class MethodChaining {
    public int methodA(int a) {
        return a * 2;
    }

    public int methodB(int b) {
        return b + 10;
    }

    public int methodC(int c) {
        return c / 3;
    }

    public int chainCalls(int value) {
        int temp1;
        int temp2;
        int result;

        // Chaining de m√©todos: methodC(methodB(methodA(value)))
        temp1 = this.methodA(value);
        temp2 = this.methodB(temp1);
        result = this.methodC(temp2);

        return result;
    }

    public static void main(String[] args) {

    }
}