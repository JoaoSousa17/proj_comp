class NestedLoops {
    public int multiplyTable(int n) {
        int i;
        int j;
        int sum;

        i = 1;
        sum = 0;

        while (i <= n) {
            j = 1;
            while (j <= n) {
                sum = sum + i * j;
                j = j + 1;
            }
            i = i + 1;
        }

        return sum;
    }

