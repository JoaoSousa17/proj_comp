import io;

class Complex {
    public int processWithBreaks(int n) {
        int i;
        int result;

        i = 0;
        result = 0;

        while (i < n) {
            // Testa múltiplas condições
            if (i % 3 == 0) {
                // Divisível por 3
                result = result + i;
            } else if (i % 3 == 1) {
                // Resto 1 na divisão por 3
                if (i > 10) {
                    i = i + 1; // Pula para o próximo
                    if (i >= n) {
                        i = n; // Simula um break
                    }
                }
            } else {
                // Resto 2 na divisão por 3
                result = result + i * 2;
            }

            i = i + 1;
        }

        return result;
    }

    public static void main(String[] args) {

    }
}