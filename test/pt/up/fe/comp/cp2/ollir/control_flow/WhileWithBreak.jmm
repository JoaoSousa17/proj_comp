class WhileWithBreak {
    public int count(int max) {
        int i;
        int sum;

        i = 0;
        sum = 0;

        while (i < 100) {
            sum = sum + i;
            i = i + 1;

            if (sum > max) {
                i = 100; // Simula um "break"
            }
        }

        return sum;
    }

    public static void main(String[] args) {

    }
}