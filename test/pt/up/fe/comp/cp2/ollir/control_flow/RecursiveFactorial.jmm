class RecursiveFactorial {
    public int compute(int n) {
        int result;

        if (n <= 1) {
            result = 1;
        } else {
            result = n * this.compute(n - 1);
        }

        return result;
    }

    public static void main(String[] args) {
        RecursiveFactorial f;
        int result;

        f = new RecursiveFactorial();
        result = f.compute(5);
    }
}