class BinarySearch {
    public int search(int[] arr, int target, int low, int high) {
        int mid;
        int value;

        if (high < low) {
            return -1; // NÃ£o encontrado
        }

        mid = low + (high - low) / 2;
        value = arr[mid];

        if (value == target) {
            return mid;
        } else if (value > target) {
            return this.search(arr, target, low, mid - 1);
        } else {
            return this.search(arr, target, mid + 1, high);
        }
    }

    public static void main(String[] args) {

    }
}