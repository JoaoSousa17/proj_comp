class Sorting {
    // Implementação de selection sort
    public int[] sort(int[] arr) {
        int i;
        int j;
        int minIndex;
        int temp;
        int length;

        length = arr.length;

        i = 0;
        while (i < length - 1) {
            minIndex = i;
            j = i + 1;

            while (j < length) {
                if (arr[j] < arr[minIndex]) {
                    minIndex = j;
                }
                j = j + 1;
            }

            // Trocar elementos
            temp = arr[minIndex];
            arr[minIndex] = arr[i];
            arr[i] = temp;

            i = i + 1;
        }

        return arr;
    }

    public static void main(String[] args) {

    }
}